            android:paddingLeft="15dp"
            android:paddingRight="15dp"
            android:paddingBottom="15dp"
            android:text="스파이시 바비큐"
            android:textColor="#383838"
            android:textSize="14sp" />

        <TextView
            android:id="@+id/spicyItalian"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="onClick"
            android:paddingLeft="15dp"
            android:paddingRight="15dp"
            android:paddingBottom="15dp"
            android:text="스파이시 이탈리안"
            android:textColor="#383838"
            android:textSize="14sp" />

        <TextView
            android:id="@+id/steaknCheese"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="onClick"
            android:paddingLeft="15dp"
            android:paddingRight="15dp"
            android:paddingBottom="15dp"
            android:text="스테이크 &amp; 치즈"
            android:textColor="#383838"
            android:textSize="14sp" />

        <TextView
            android:id="@+id/subwayClub"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="onClick"
            android:paddingLeft="15dp"
            android:paddingRight="15dp"
            android:paddingBottom="15dp"
            android:text="써브웨이 클럽"
            android:textColor="#383838"
            android:textSize="14sp" />

        <TextView
            android:id="@+id/tuna"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="onClick"
            android:paddingLeft="15dp"
            android:paddingRight="15dp"
            android:paddingBottom="15dp"
            android:text="참치"
            android:textColor="#383838"
            android:textSize="14sp" />

        <TextView
            android:id="@+id/vege"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="onClick"
            android:paddingLeft="15dp"
            android:paddingRight="15dp"
            android:paddingBottom="15dp"
            android:text="베지"
            android:textColor="#383838"
            android:textSize="14sp" />

