<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/sandwich_list"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <LinearLayout
        android:id="@+id/sandwich_menu_list"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#F3F3F3"
        android:orientation="vertical"
        android:paddingTop="15dp">

        <TextView
            android:id="@+id/BLT"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="onClick"
            android:paddingLeft="15dp"
            android:paddingRight="15dp"
            android:paddingBottom="15dp"
            android:text="비엘티"
            android:textColor="#383838"
            android:textSize="14sp" />

        <TextView
            android:id="@+id/KBBQ"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="onClick"
            android:paddingLeft="15dp"
            android:paddingRight="15dp"
            android:paddingBottom="15dp"
            android:text="K-바베큐"
            android:textColor="#383838"
            android:textSize="14sp" />

        <TextView
            android:id="@+id/chickenBaconAvocado"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="onClick"
            android:paddingLeft="15dp"
            android:paddingRight="15dp"
            android:paddingBottom="15dp"
            android:text="치킨 베이컨 아보카도"
            android:textColor="#383838"
            android:textSize="14sp" />

        <TextView
            android:id="@+id/chickenSlice"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="onClick"
            android:paddingLeft="15dp"
            android:paddingRight="15dp"
            android:paddingBottom="15dp"
            android:text="치킨 슬라이스"
            android:textColor="#383838"
            android:textSize="14sp" />

        <TextView
            android:id="@+id/chickenTeriyaki"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="onClick"
            android:paddingLeft="15dp"
            android:paddingRight="15dp"
            android:paddingBottom="15dp"
            android:text="치킨 데리야끼"
            android:textColor="#383838"
            android:textSize="14sp" />

        <TextView
            android:id="@+id/eggMayo"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="onClick"
            android:paddingLeft="15dp"
            android:paddingRight="15dp"
            android:paddingBottom="15dp"
            android:text="에그마요"
            android:textColor="#383838"
            android:textSize="14sp" />

        <TextView
